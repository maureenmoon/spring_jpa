package study02;

public class BreakTest {

	public static void main(String[] args) {
		
		int sum = 0;
		
		for (int n = 1;n<=10;n++) {		
			sum += n;
			
			if (n==5) break;		
		}
		System.out.println(sum);
		
		
		for (int n=1;n<=100;i++) {
			if(n%3 ==0) continue;
			System.out.println(n);
		}
	}

}
